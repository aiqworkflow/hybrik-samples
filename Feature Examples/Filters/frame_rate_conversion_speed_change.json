{
  "definitions": {
    "descriptor": "speed change filter",
    "source_0_path": "https://hybrik-examples.s3.amazonaws.com/public/sources",
    "source_0_file": "sample1.mp4",
    "destination_path": "s3://your-bucket/tutorials/optical_flow",
    "output_filename": "output"
  },
  "name": "{{descriptor}}",
  "payload": {
    "elements": [
      {
        "uid": "source",
        "kind": "source",
        "payload": {
          "kind": "asset_url",
          "payload": {
            "storage_provider": "http",
            "url": "{{source_0_path}}/{{source_0_file}}"
          }
        }
      },
      {
        "uid": "transcode_task",
        "kind": "transcode",
        "task": {
          "retry_method": "fail"
        },
        "payload": {
          "location": {
            "storage_provider": "s3",
            "path": "{{destination_path}}"
          },
          "options": {
            "pipeline": {
              "encoder_version": "hybrik_4.2_8bit"
            }
          },
          "targets": [
            {
              "file_pattern": "{{output_filename}}{default_extension}",
              "existing_files": "rename_new",
              "container": {
                "kind": "mp4",
                "filters": [
                  {
                    "kind": "speed_change",
                    "payload": {
                      "factor": "25/(24000/1001)",
                      "pitch_correction": false
                    }
                  }
                ]
              },
              "video": {
                "codec": "h264",
                "width": 640,
                "height": 360,
                "frame_rate": 25,
                "profile": "main",
                "level": "4.0",
                "chroma_format": "yuv420p",
                "bitrate_kb": 4000,
                "bitrate_mode": "cbr",
                "x264_options": "force-cfr=1:b_pyramid=1:nal-hrd=CBR:b_adapt=1"
              },
              "audio": [
                {
                  "codec": "aac",
                  "channels": 2,
                  "sample_rate": 48000,
                  "sample_size": 24,
                  "bitrate_kb": 128
                }
              ]
            }
          ]
        }
      }
    ],
    "connections": [
      {
        "from": [
          {
            "element": "source"
          }
        ],
        "to": {
          "success": [
            {
              "element": "transcode_task"
            }
          ]
        }
      }
    ]
  }
}